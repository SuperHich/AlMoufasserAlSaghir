<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mostafad_layout"
    android:layout_width="1422px"
    android:layout_height="800px"
     android:background="@drawable/e5_mostafad_bg">
    
    
    <WebView
        android:id="@+id/mostafad_content"
        android:layout_width="1071px"
        android:layout_height="391px"
       android:layout_marginLeft="150px"
       android:layout_marginTop="250px"
      />
    
    
    <Button 
	    android:id="@+id/mostafad_previous"
        android:layout_width="81px"
        android:layout_height="81px"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="150px"
       android:layout_marginBottom="100px"
        android:background="@drawable/previous" 
	    />

</RelativeLayout>
